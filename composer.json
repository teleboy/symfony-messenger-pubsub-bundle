{
    "name": "teleboy/symfony-messenger-pubsub-bundle",
    "description": "Symfony Messenger Bundle adding Google Cloud Pub/Sub support",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Cedric Ziel",
            "email": "mail@cedric-ziel.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.teleboy.ch/"
        }
    ],
    "autoload": {
        "psr-4": {
            "CedricZiel\\Symfony\\Bundle\\GoogleCloudPubSubMessenger\\": ""
        }
    },
    "require": {
        "php": ">=7.4",
        "teleboy/symfony-messenger-pubsub-bridge": "^0.3",
        "psr/log": "^1.0",
        "symfony/config": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.3",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/http-foundation": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/messenger": "^5.0",
        "symfony/serializer": "^4.4|^5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^4.4 || ^5.0",
        "symfony/phpunit-bridge": "^4.4 || ^5.0",
        "symfony/translation": "^5.3",
        "symfony/validator": "^5.3",
        "symfony/yaml": "^4.4 || ^5.0"
    },
    "scripts": {
        "test": "@php vendor/bin/simple-phpunit",
        "phpstan": "@php -d memory_limit=-1 vendor/phpstan/phpstan/phpstan analyse"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
